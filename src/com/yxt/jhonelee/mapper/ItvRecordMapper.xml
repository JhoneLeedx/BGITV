<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yxt.jhonelee.mapper.AdminMapper">
	<resultMap type="ITVRecord" id="ITVRECORDMAP">
		<id property="mId" column="id" />
		<result property="mAdminId" column="admin_id" />
		<result property="mRegistId" column="regist_id" />
		<result property="mReason" column="reason" />
		<result property="mHandle" column="handle"/>
	</resultMap>

	<select id="SelectAllRecord" resultType="ITVRecord">
		select * from ITV_RECORD where regist_id = #{id}
	</select>

	<insert id="InsertRecord" keyProperty="mId" timeout="5000"
		parameterType="ITVRecord">
		insert into ITV_RECORD (admin_id,REGIST_ID,reason,handle)
		values(#{mAdminId}, #{mRegistId}, #{mReason},#{mHandle})
	</insert>

</mapper>